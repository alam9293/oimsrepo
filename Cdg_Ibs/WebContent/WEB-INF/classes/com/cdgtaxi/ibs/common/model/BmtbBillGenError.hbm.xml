<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.BmtbBillGenError" 
    table="BMTB_BILL_GEN_ERROR"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="errorNo"
        type="java.lang.Long"
        column="ERROR_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">BMTB_BILL_GEN_ERROR_SQ1</param>
        </generator>
    </id>

    <property
        name="errorMsg"
        type="java.lang.String"
        column="ERROR_MSG"
        length="255"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="errorDt"
        type="java.sql.Timestamp"
        column="ERROR_DT"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="vdFlag"
        type="java.lang.String"
        column="VD_FLAG"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="rwdFlag"
        type="java.lang.String"
        column="RWD_FLAG"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one
        name="amtbAccountByTopLevelAccountNo"
        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="TOP_LEVEL_ACCOUNT_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one
        name="amtbAccountByBillableAccountNo"
        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="BILLABLE_ACCOUNT_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to BmtbBillGenReq -->
    <many-to-one
        name="bmtbBillGenReq"
        class="com.cdgtaxi.ibs.common.model.BmtbBillGenReq"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="REQ_NO" />
    </many-to-one>

</class>
</hibernate-mapping>
