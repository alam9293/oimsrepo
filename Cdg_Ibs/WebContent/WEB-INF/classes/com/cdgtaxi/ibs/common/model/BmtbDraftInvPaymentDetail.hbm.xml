<class 
    name="com.cdgtaxi.ibs.common.model.BmtbDraftInvPaymentDetail" 
    table="BMTB_DRAFT_INV_PAYMENT_DETAIL"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="invoicePaymentDetailNo"
        type="java.lang.Long"
        column="INVOICE_PAYMENT_DETAIL_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">BMTB_DRAFT_INV_PYMT_DETAIL_SQ1</param>
        </generator>
    </id>

    <property
        name="invoiceNo"
        type="long"
        column="INVOICE_NO"
        not-null="true"
        length="18"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="accountName"
        type="java.lang.String"
        column="ACCOUNT_NAME"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="paymentAmount"
        type="java.math.BigDecimal"
        column="PAYMENT_AMOUNT"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to BmtbDraftInvPaymentHeader -->
    <many-to-one
        name="bmtbDraftInvPaymentHeader"
        class="com.cdgtaxi.ibs.common.model.BmtbDraftInvPaymentHeader"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="INVOICE_PAYMENT_NO" />
    </many-to-one>

