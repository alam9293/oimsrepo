<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.BmtbLastInvoiceLog" 
    table="BMTB_LAST_INVOICE_LOG"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="accountNo"
        type="java.lang.Integer"
        column="ACCOUNT_NO"
    >
        <meta attribute="use-in-equals">true</meta>
    </id>

    <property
        name="billGenDate"
        type="java.sql.Date"
        column="BILL_GEN_DATE"
        not-null="true"
        length="7"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to AmtbAccount -->
    <one-to-one
        name="amtbAccount"
        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to BmtbInvoiceHeader -->
    <many-to-one
        name="bmtbInvoiceHeader"
        class="com.cdgtaxi.ibs.common.model.BmtbInvoiceHeader"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="INVOICE_HEADER_NO" />
    </many-to-one>

</class>
</hibernate-mapping>
