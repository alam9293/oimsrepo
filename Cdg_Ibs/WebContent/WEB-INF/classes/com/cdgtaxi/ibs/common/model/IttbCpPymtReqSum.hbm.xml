<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class 
    name="com.cdgtaxi.ibs.common.model.IttbCpPymtReqSum" 
    table="ITTB_CP_PYMT_REQ_SUM"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

      <id
        name="pymtReqId"
        type="java.lang.Integer"
        column="PYMT_REQ_ID"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">ITTB_CP_PYMT_REQ_SUM_SQ1</param>
        </generator>
    </id>   
	<version name="version" column="VERSION"/>

    <property
        name="loginId"
        type="java.lang.String"
        column="LOGIN_ID"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
     <property
        name="loginNo"
        type="java.lang.Integer"
        column="LOGIN_NO"
        not-null="true"
        length="8"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="mid"
        type="java.lang.String"
        column="MID"
        not-null="true"
        length="20"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="refNo"
        type="java.lang.String"
        column="REF_NO"
        not-null="true"
        length="40"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="currency"
        type="java.lang.String"
        column="CURRENCY"
        not-null="true"
        length="3"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="topupAmount"
        type="java.math.BigDecimal"
        column="TOPUP_AMOUNT"
        not-null="true"
        length="10"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        not-null="true"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DATE"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to PmtbProduct -->
