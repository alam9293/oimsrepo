        name="ittbCpLogin"
        class="com.cdgtaxi.ibs.common.model.IttbCpLogin"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="LOGIN_NO" />
    </many-to-one> -->
    

</class>
</hibernate-mapping>
