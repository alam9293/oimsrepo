<?xml version="1.0" encoding="UTF-8"?>

<window id="manageMasterLoginWindow" border="normal" use="com.cdgtaxi.ibs.portal.ui.ManageMasterLoginWindow"
	title="Manage Master Login"
	xmlns:h="http://www.w3.org/1999/xhtml">

	<grid>
		<columns sizable="false">
			<column width="25%"/>
			<column width="25%"/>
			<column width="25%"/>
			<column width="25%"/>
		</columns>
   		<rows>
   			<row>
   				<label sclass="fieldLabel">Master Login Id:</label>
   				<capstextbox id="masterLoginIdTextBox" maxlength="80" width="98%"/>
   				
	   			<button label="Search" onClick="manageMasterLoginWindow.search()" />
