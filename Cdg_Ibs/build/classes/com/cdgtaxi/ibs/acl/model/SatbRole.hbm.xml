<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.acl.model.SatbRole" 
    table="SATB_ROLE"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="roleId"
        type="java.lang.Long"
        column="ROLE_ID"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">SATB_ROLE_SQ1</param>
        </generator>
    </id>
	<version name="version" column="VERSION"/>

    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        not-null="true"
        unique="true"
        length="64"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-many association to SatbUser -->
    <set
        name="satbUsers"
        lazy="true"
		cascade="all"
        table="SATB_USER_ROLE"
    >
        <key>
            <column name="ROLE_ID" />
        </key>
        <many-to-many
            class="com.cdgtaxi.ibs.acl.model.SatbUser"
        >
            <column name="USER_ID" />
        </many-to-many>
    </set>
    <!-- bi-directional many-to-many association to SatbResource -->
    <set
        name="satbResources"
        lazy="true"
		cascade="all"
        table="SATB_ROLE_RESOURCE"
    >
        <key>
            <column name="ROLE_ID" />
        </key>
        <many-to-many
            class="com.cdgtaxi.ibs.acl.model.SatbResource"
        >
            <column name="RSRC_ID" />
        </many-to-many>
    </set>

</class>
</hibernate-mapping>
