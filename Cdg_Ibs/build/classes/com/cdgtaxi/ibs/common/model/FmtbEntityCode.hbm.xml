<class 
    name="com.cdgtaxi.ibs.common.model.FmtbEntityCode" 
    table="FMTB_ENTITY_CODE"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="entityCodeNo"
        type="java.lang.Integer"
        column="ENTITY_CODE_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">FMTB_ENTITY_CODE_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="glCoCode"
        type="java.lang.String"
        column="GL_CO_CODE"
        not-null="true"
        length="10"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="glCurrencyCode"
        type="java.lang.String"
        column="GL_CURRENCY_CODE"
        not-null="true"
        length="3"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="effectiveDate"
        type="java.sql.Date"
        column="EFFECTIVE_DATE"
        not-null="true"
        length="7"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to FmtbEntityMaster -->
    <many-to-one
        name="fmtbEntityMaster"
        class="com.cdgtaxi.ibs.common.model.FmtbEntityMaster"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ENTITY_NO" />
    </many-to-one>

</class>
</hibernate-mapping>
