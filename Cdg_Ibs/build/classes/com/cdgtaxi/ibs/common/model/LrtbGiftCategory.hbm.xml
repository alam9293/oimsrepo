<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.LrtbGiftCategory" 
    table="LRTB_GIFT_CATEGORY"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="giftCategoryNo"
        type="java.lang.Integer"
        column="GIFT_CATEGORY_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">LRTB_GIFT_CATEGORY_SQ1</param>
        </generator>
    </id>
	<version name="version" column="VERSION"/>

    <property
        name="categoryName"
        type="java.lang.String"
        column="CATEGORY_NAME"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to LrtbGiftItem -->
    <set
        name="lrtbGiftItems"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="GIFT_CATEGORY_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.LrtbGiftItem"
        />
    </set>

</class>
</hibernate-mapping>
