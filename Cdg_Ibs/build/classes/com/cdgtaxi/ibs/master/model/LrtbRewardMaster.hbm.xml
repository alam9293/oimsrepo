<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.master.model.LrtbRewardMaster" 
    table="LRTB_REWARD_MASTER"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="rewardPlanNo"
        type="java.lang.Integer"
        column="REWARD_PLAN_NO"
    >
   
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">LRTB_REWARD_MASTER_SQ1</param>
        </generator>
    </id>
 	<version name="version" column="VERSION"/>
 	
    <property
        name="rewardPlanName"
        type="java.lang.String"
        column="REWARD_PLAN_NAME"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to AmtbApplicationProduct -->
    <set
        name="amtbApplicationProducts"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="REWARDS_PLAN_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.AmtbApplicationProduct"
        />
    </set>
    <!-- bi-directional one-to-many association to LrtbRewardDetail -->
    <set
        name="lrtbRewardDetails"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="REWARD_PLAN_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.master.model.LrtbRewardDetail"
        />
    </set>

</class>
</hibernate-mapping>
