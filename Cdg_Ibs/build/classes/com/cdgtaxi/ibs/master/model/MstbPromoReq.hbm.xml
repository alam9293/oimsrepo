<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.master.model.MstbPromoReq" 
    table="MSTB_PROMO_REQ"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="promoReqNo"
        type="java.lang.Integer"
        column="PROMO_REQ_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">MSTB_PROMO_REQ_SQ1</param>
        </generator>
    </id>
    <property
        name="event"
        type="java.lang.String"
        column="EVENT"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
     <property
        name="requestBy"
        type="java.lang.String"
        column="REQUEST_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="requestDt"
        type="java.sql.Timestamp"
        column="REQUEST_DT"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="version"
        type="java.lang.Integer"
        column="VERSION"
        length="9"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
   	<!-- bi-directional one-to-one association to MstbPromoDetail -->
    <many-to-one
        name="mstbPromotion"
        class="com.cdgtaxi.ibs.master.model.MstbPromotion"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="PROMO_NO" />
    </many-to-one>
    <many-to-one
        name="fromPromoDetail"
        class="com.cdgtaxi.ibs.master.model.MstbPromoDetail"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="FROM_PROMO_DETAIL_NO" />
    </many-to-one>
    <many-to-one
        name="lastPromoReqFlow"
        class="com.cdgtaxi.ibs.master.model.MstbPromoReqFlow"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="LAST_PROMO_REQ_FLOW_NO" />
    </many-to-one>
    <set
        name="mstbPromoReqFlows"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="PROMO_REQ_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.master.model.MstbPromoReqFlow"
        />
    </set>
   
</class>
</hibernate-mapping>
