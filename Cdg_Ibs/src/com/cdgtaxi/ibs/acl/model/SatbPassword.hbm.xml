<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.acl.model.SatbPassword" 
    table="SATB_PASSWORD"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="passwordId"
        type="java.lang.Long"
        column="PASSWORD_ID"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">SATB_PASSWORD_SQ1</param>
        </generator>
    </id>

    <property
        name="password"
        type="java.lang.String"
        column="PASSWORD"
        length="64"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="time"
        type="java.sql.Timestamp"
        column="TIME"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to SatbUser -->
    <many-to-one
        name="satbUser"
        class="com.cdgtaxi.ibs.acl.model.SatbUser"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="USER_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
