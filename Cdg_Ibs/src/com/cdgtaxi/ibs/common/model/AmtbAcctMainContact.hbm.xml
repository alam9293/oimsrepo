<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.AmtbAcctMainContact" 
    table="AMTB_ACCT_MAIN_CONTACT"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <composite-id name="comp_id" class="com.cdgtaxi.ibs.common.model.AmtbAcctMainContactPK">
        <meta attribute="use-in-equals">true</meta>
        <key-property 
            name="mainContactType" 
            column="MAIN_CONTACT_TYPE" 
            type="java.lang.String"
            length="1"
        />
        <!-- bi-directional many-to-one association to AmtbAccount -->
        <key-many-to-one
        	name="amtbAccount"
	        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
	    >
    	    <column name="ACCOUNT_NO" />
	    </key-many-to-one>
    </composite-id>
    <version name="version" column="VERSION"/>   

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AmtbContactPerson -->
    <many-to-one
        name="amtbContactPerson"
        class="com.cdgtaxi.ibs.common.model.AmtbContactPerson"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="CONTACT_PERSON_NO" />
    </many-to-one>

</class>
</hibernate-mapping>
