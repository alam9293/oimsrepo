<sql-query name="getLatestTaxRate">
<return-scalar column="tax_rate" type="java.math.BigDecimal"/>
 <![CDATA[
	select tax_rate
	from
	(
	    select *
	    from fmtb_tax_code tax
	    inner join (
	        select *
	        from(
	          select acct.account_no, ftc.entity_no, ftc.tax_type
	          from amtb_account acct
	          inner join fmtb_ar_cont_code_master faccm on acct.AR_CONTROL_CODE_NO = faccm.AR_CONTROL_CODE_NO
	          inner join fmtb_transaction_code ftc on ftc.entity_no = faccm.entity_no
	          where 
	            acct.account_no = :topLevelAccountNo
	            and
	            	--for those product type that is null, pass in "NULL" for comparison
              		(ftc.product_type_id = :productTypeId or NVL(ftc.product_type_id,'NULL') = :productTypeId)
	            and ftc.txn_type = :txnType
	            and ftc.effective_date <= :date
	          order by ftc.effective_date desc
	        ) temp1
	        where rownum = 1
	    ) latest_tax_type 
	      on tax.entity_no = latest_tax_type.entity_no
	      and tax.tax_type = latest_tax_type.tax_type
	    where tax.effective_date <= :date
	    order by effective_date desc
	)
	where rownum = 1
]]>
</sql-query>

</hibernate-mapping>
