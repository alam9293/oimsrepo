<class 
    name="com.cdgtaxi.ibs.common.model.IttbRecurringChargeTagAcct" 
    table="ITTB_RECURRING_CHARGE_TAG_ACCT"
    dynamic-update="true"
    dynamic-insert="true"
>


    <meta attribute="implement-equals" inherit="false">true</meta>
<id
        name="id"
        type="java.lang.Integer"
        column="ID"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence">ITSQ_RECURRING_CHARGE_TG_ACCT</param>
        </generator>
    </id>
   
   <!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one name="amtbAccount" class="com.cdgtaxi.ibs.common.model.AmtbAccount" fetch="join">
            <column name="ACCOUNT_NO" />
        </many-to-one>
        
        
	<!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one name="recurringChargeId" class="com.cdgtaxi.ibs.common.model.IttbRecurringCharge" fetch="join">
            <column name="RECURRING_CHARGE_ID" />
        </many-to-one>
        
</class>
</hibernate-mapping>
