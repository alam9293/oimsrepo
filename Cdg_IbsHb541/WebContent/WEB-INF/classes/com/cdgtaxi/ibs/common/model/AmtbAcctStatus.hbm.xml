<class 
    name="com.cdgtaxi.ibs.common.model.AmtbAcctStatus" 
    table="AMTB_ACCT_STATUS"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="acctStatusNo"
        type="java.lang.Integer"
        column="ACCT_STATUS_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">AMTB_ACCT_STATUS_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="effectiveDt"
        type="java.sql.Timestamp"
        column="EFFECTIVE_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="statusRemarks"
        type="java.lang.String"
        column="STATUS_REMARKS"
        length="500"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="acctStatus"
        type="java.lang.String"
        column="ACCT_STATUS"
        not-null="true"
        length="2"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to MstbMasterTable -->
    <many-to-one
        name="mstbMasterTable"
        class="com.cdgtaxi.ibs.master.model.MstbMasterTable"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="STATUS_REASON" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one
        name="amtbAccount"
        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ACCOUNT_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to SatbUser -->
    <many-to-one
        name="satbUser"
        class="com.cdgtaxi.ibs.acl.model.SatbUser"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="USER_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
