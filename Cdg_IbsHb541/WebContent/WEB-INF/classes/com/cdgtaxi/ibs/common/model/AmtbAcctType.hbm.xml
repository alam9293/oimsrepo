<class 
    name="com.cdgtaxi.ibs.common.model.AmtbAcctType" 
    table="AMTB_ACCT_TYPE"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="acctTypeNo"
        type="java.lang.Integer"
        column="ACCT_TYPE_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">AMTB_ACCT_TYPE_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="acctType"
        type="java.lang.String"
        column="ACCT_TYPE"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="acctTemplate"
        type="java.lang.String"
        column="ACCT_TEMPLATE"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to AmtbApplication -->
    <set
        name="amtbApplications"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="ACCT_TYPE_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.AmtbApplication"
        />
    </set>
    <!-- bi-directional one-to-many association to AmtbAccount -->
    <set
        name="amtbAccounts"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="ACCT_TYPE_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        />
    </set>
    <!-- bi-directional many-to-many association to PmtbProductType -->
    <set
        name="pmtbProductTypes"
        lazy="true"
		cascade="all"
        table="AMTB_SUBSC_PROD_TYPE"
    >
        <key>
            <column name="ACCT_TYPE_NO" />
        </key>
        <many-to-many
            class="com.cdgtaxi.ibs.common.model.PmtbProductType"
        >
            <column name="PRODUCT_TYPE_ID" />
        </many-to-many>
    </set>

</class>
</hibernate-mapping>
