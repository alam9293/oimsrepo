<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.BmtbDraftInvPaymentHeader" 
    table="BMTB_DRAFT_INV_PAYMENT_HEADER"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="invoicePaymentNo"
        type="java.lang.Long"
        column="INVOICE_PAYMENT_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">BMTB_DRAFT_INV_PYMT_HEADER_SQ1</param>
        </generator>
    </id>

    <property
        name="headerType"
        type="java.lang.String"
        column="HEADER_TYPE"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="paymentType"
        type="java.lang.String"
        column="PAYMENT_TYPE"
        not-null="true"
        length="50"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="paymentRefNo"
        type="java.lang.String"
        column="PAYMENT_REF_NO"
        length="30"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="paymentAmount"
        type="java.math.BigDecimal"
        column="PAYMENT_AMOUNT"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to BmtbDraftInvHeader -->
    <many-to-one
        name="bmtbDraftInvHeader"
        class="com.cdgtaxi.ibs.common.model.BmtbDraftInvHeader"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="INVOICE_HEADER_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to BmtbDraftInvPaymentDetail -->
    <set
        name="bmtbDraftInvPaymentDetails"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="INVOICE_PAYMENT_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.BmtbDraftInvPaymentDetail"
        />
    </set>

</class>
</hibernate-mapping>
