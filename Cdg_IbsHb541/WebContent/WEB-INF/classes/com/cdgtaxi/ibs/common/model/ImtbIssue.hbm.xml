<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.ImtbIssue" 
    table="IMTB_ISSUE"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="issueNo"
        type="java.lang.Integer"
        column="ISSUE_NO"
    >
        <meta attribute="use-in-equals">true</meta>
		<generator class="assigned"></generator>
<!--        <generator class="sequence">-->
<!--            <param name="sequence_name">IMTB_ISSUE_SQ1</param>-->
<!--        </generator>-->
    </id>

    <property
        name="expiryDate"
        type="java.sql.Date"
        column="EXPIRY_DATE"
        length="7"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="handlingFee"
        type="java.math.BigDecimal"
        column="HANDLING_FEE"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="discount"
        type="java.math.BigDecimal"
        column="DISCOUNT"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="deliveryCharges"
        type="java.math.BigDecimal"
        column="DELIVERY_CHARGES"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="version"
        type="java.lang.Integer"
        column="VERSION"
        length="9"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to ImtbIssueReq -->
    <one-to-one
        name="imtbIssueReq"
        class="com.cdgtaxi.ibs.common.model.ImtbIssueReq"
        outer-join="auto"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to ImtbStock -->
    <many-to-one
        name="imtbStock"
        class="com.cdgtaxi.ibs.common.model.ImtbStock"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="STOCK_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to ImtbItem -->
    <set
        name="imtbItems"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="ISSUE_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.ImtbItem"
        />
    </set>
    <!-- bi-directional many-to-one association to SatbUser -->
    <many-to-one
        name="satbUser"
        class="com.cdgtaxi.ibs.acl.model.SatbUser"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="USER_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
