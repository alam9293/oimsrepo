<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.ImtbStock" 
    table="IMTB_STOCK"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="stockNo"
        type="java.lang.Integer"
        column="STOCK_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">IMTB_STOCK_SQ1</param>
        </generator>
    </id>

    <property
        name="txnType"
        type="java.lang.String"
        column="TXN_TYPE"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="txnDt"
        type="java.sql.Timestamp"
        column="TXN_DT"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="serialNoStart"
        type="java.math.BigDecimal"
        column="SERIAL_NO_START"
        not-null="true"
        length="25"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="serialNoEnd"
        type="java.math.BigDecimal"
        column="SERIAL_NO_END"
        not-null="true"
        length="25"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="version"
        type="java.lang.Integer"
        column="VERSION"
        length="9"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to ImtbItemType -->
    <many-to-one
        name="imtbItemType"
        class="com.cdgtaxi.ibs.common.model.ImtbItemType"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ITEM_TYPE_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to ImtbIssue -->
    <set
        name="imtbIssues"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="STOCK_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.ImtbIssue"
        />
    </set>

</class>
</hibernate-mapping>
