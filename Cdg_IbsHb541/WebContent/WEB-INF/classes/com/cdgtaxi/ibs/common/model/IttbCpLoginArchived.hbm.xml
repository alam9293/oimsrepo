<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>

<class 
    name="com.cdgtaxi.ibs.common.model.IttbCpLoginArchived" 
    table="ITTB_CP_LOGIN_ARCHIVED"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="archivedNo"
        type="java.lang.Long"
        column="ARCHIVED_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">ITTB_CP_LOGIN_ARCHIVED_SQ1</param>
        </generator>
    </id>

     <property
         	name="loginId" 
            column="LOGIN_ID" 
            type="java.lang.String"
            not-null="true"
            length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
         	name="loginType" 
            column="LOGIN_TYPE" 
            type="java.lang.String"
            not-null="true"
            length="3"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>    
    
    <property
        name="loginMethod"
        type="java.lang.String"
        column="LOGIN_METHOD"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
        <property
        name="productNo"
        type="java.math.BigDecimal"
        column="PRODUCT_NO"
        length="19"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
        name="contactPersonNo"
        type="java.lang.Integer"
        column="CONTACT_PERSON_NO"
        length="8"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
      <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
        <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>


</class>
</hibernate-mapping>
