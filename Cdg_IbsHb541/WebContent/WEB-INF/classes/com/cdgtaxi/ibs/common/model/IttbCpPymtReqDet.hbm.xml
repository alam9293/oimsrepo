<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class 
    name="com.cdgtaxi.ibs.common.model.IttbCpPymtReqDet" 
    table="ITTB_CP_PYMT_REQ_DET"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

      <id
        name="pymtReqDetId"
        type="java.lang.Integer"
        column="PYMT_REQ_DET_ID"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">ITTB_CP_PYMT_REQ_SUM_SQ1</param>
        </generator>
    </id>   
	<version name="version" column="VERSION"/>

    <property
        name="topupAmount"
        type="java.math.BigDecimal"
        column="TOPUP_AMOUNT"
        not-null="true"
        length="10"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="nameOnProduct"
        type="java.lang.String"
        column="NAME_ON_PRODUCT"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="cardNo"
        type="java.lang.String"
        column="CARD_NO"
        not-null="true"
        length="19"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="creditBalance"
        type="java.math.BigDecimal"
        column="CREDIT_BALANCE"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="expiryDate"
        type="java.util.Date"
        column="EXPIRY_DATE"
        not-null="true"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DATE"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DATE"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to PmtbProduct -->
    <many-to-one
        name="pmtbProduct"
        class="com.cdgtaxi.ibs.common.model.PmtbProduct"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="PRODUCT_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AmtbContactPerson -->
    <many-to-one
        name="pmtbProductType"
        class="com.cdgtaxi.ibs.common.model.PmtbProductType"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="PRODUCT_TYPE_ID" />
    </many-to-one>
    <many-to-one
        name="ittbCpPymtReqSum"
        class="com.cdgtaxi.ibs.common.model.IttbCpPymtReqSum"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="PYMT_REQ_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
