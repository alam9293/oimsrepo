<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.LrtbGiftItem" 
    table="LRTB_GIFT_ITEM"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="giftItemNo"
        type="java.lang.Integer"
        column="GIFT_ITEM_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">LRTB_GIFT_ITEM_SQ1</param>
        </generator>
    </id>
	<version name="version" column="VERSION"/>

    <property
        name="itemCode"
        type="java.lang.String"
        column="ITEM_CODE"
        not-null="true"
        length="20"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="itemName"
        type="java.lang.String"
        column="ITEM_NAME"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="price"
        type="java.math.BigDecimal"
        column="PRICE"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="stock"
        type="java.lang.Integer"
        column="STOCK"
        not-null="true"
        length="12"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="image"
        type="java.sql.Blob"
        column="IMAGE"
        length="4000"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="points"
        type="java.lang.Integer"
        column="POINTS"
        not-null="true"
        length="8"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to LrtbGiftCategory -->
    <many-to-one
        name="lrtbGiftCategory"
        class="com.cdgtaxi.ibs.common.model.LrtbGiftCategory"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="GIFT_CATEGORY_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to LrtbGiftStock -->
    <set
        name="lrtbGiftStocks"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="GIFT_ITEM_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.LrtbGiftStock"
        />
    </set>

</class>
</hibernate-mapping>
