<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.LrtbRewardAccount" 
    table="LRTB_REWARD_ACCOUNT"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="rewardAccountNo"
        type="java.lang.Integer"
        column="REWARD_ACCOUNT_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">LRTB_REWARD_ACCOUNT_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="cutOffDt"
        type="java.sql.Timestamp"
        column="CUT_OFF_DT"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="expireDt"
        type="java.sql.Timestamp"
        column="EXPIRE_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="ibsExpireDt"
        type="java.sql.Timestamp"
        column="IBS_EXPIRE_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="extMthToExpiry"
        type="java.lang.Integer"
        column="EXT_MTH_TO_EXPIRY"
        length="2"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="expiredBilledFlag"
        type="java.lang.String"
        column="EXPIRED_BILLED_FLAG"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one
        name="amtbAccount"
        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ACCOUNT_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to LrtbRewardTxn -->
    <set
        name="lrtbRewardTxns"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="REWARD_ACCOUNT_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.LrtbRewardTxn"
        />
    </set>

</class>
</hibernate-mapping>
