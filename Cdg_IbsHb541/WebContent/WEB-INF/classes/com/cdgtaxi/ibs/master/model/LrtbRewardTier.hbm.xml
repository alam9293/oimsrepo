    table="LRTB_REWARD_TIER"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="rewardPlanTierNo"
        type="java.lang.Integer"
        column="REWARD_PLAN_TIER_NO"
    >
