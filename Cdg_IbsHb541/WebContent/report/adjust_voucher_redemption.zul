<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>

<window id="adjustVoucherRedemptionWin" border="normal" use="com.cdgtaxi.ibs.report.ui.AdjustVoucherRedemptionWindow"
	title="Adjust Voucher Redemption"
	xmlns:h="http://www.w3.org/1999/xhtml">

	<grid>
		<columns sizable="false">
			<column width="30%"/>
			<column width="70%"/>
		</columns>
   		<rows>
        <row> 
          <label sclass="fieldLabel">01. Batch No</label>
          <capstextbox id="batchNoTB" 
            width="50%" 
          />
        </row>
   			<row> 
   				<label sclass="fieldLabel">02. Action Start Date</label>
   				<datebox id="actionStartDateDB" 
            width="50%" 
            format="dd/MM/yyyy"
          />
   			</row>
   			<row> 
   				<label sclass="fieldLabel">03. Action End Date</label>
          <datebox id="actionEndDateDB" 
            width="50%" 
            format="dd/MM/yyyy"
          />
   			</row>
   			<row> 
   				<label sclass="fieldLabel">04. Approval Start Date</label>
          <datebox id="approvalStartDateDB" 
            width="50%" 
            format="dd/MM/yyyy"
          />
   			</row>
   			<row> 
   				<label sclass="fieldLabel">05. Approval End Date</label>
          <datebox id="approvalEndDateDB" 
            width="50%" 
            format="dd/MM/yyyy"
          />
   			</row>
        <row> 
          <label sclass="fieldLabel">06. Serial No Start</label>
          <decimalbox id="serialNoStartDMB" 
            width="50%" 
            format="0"
          />
        </row>
        <row> 
          <label sclass="fieldLabel">07. Serial No End</label>
          <decimalbox id="serialNoEndDMB" 
            width="50%" 
            format="0"
          />
        </row>
   			<row> 
   				<label sclass="fieldLabel">08. Item Type</label>
   				<listbox id="itemTypeLB" 
            mold="select"
            multiple="false"
            width="50%"
          />
   			</row>
        <row> 
          <label sclass="fieldLabel">09. Action Type</label>
          <listbox id="actionTypeLB" 
            mold="select"
            multiple="false"
            width="50%"
          />
        </row>
   			<row> 
   				<label sclass="fieldLabel">10. Approval Status</label>
   				<listbox id="approvalStatusLB" 
            mold="select"
            multiple="false"
            width="50%"
          />
   			</row>
   			<row>
				<label sclass="fieldLabel">11. Entity</label>
				<listbox id="entityLB" mold="select" multiple="false" width="50%" />
			</row>
   			<row> 
   				<label sclass="fieldLabel required" id="reportFormatLabel">12. Report Format:</label>
   				<listbox id="reportFormat" 
   					mold="select"
   					multiple="false"
   					onCreate="adjustVoucherRedemptionWin.populateReportFormatList(self)"
   					width="50%"
   				/>
   			</row>
   			<row spans="2">
   				<hbox>
   					<button label="Generate" onClick="adjustVoucherRedemptionWin.generate()"/>
   					<button label="Reset" onClick="adjustVoucherRedemptionWin.reset()"/>
   				</hbox>
   			</row>
   		</rows>
   	</grid>
   	
</window>
