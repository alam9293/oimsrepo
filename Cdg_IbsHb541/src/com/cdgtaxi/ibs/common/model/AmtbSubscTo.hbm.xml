<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.AmtbSubscTo" 
    table="AMTB_SUBSC_TO"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <composite-id name="comp_id" class="com.cdgtaxi.ibs.common.model.AmtbSubscToPK">
        <meta attribute="use-in-equals">true</meta>
        <!-- bi-directional many-to-one association to PmtbProductType -->
        <key-many-to-one
        	name="pmtbProductType"
	        class="com.cdgtaxi.ibs.common.model.PmtbProductType"
	    >
    	    <column name="PRODUCT_TYPE_ID" />
	    </key-many-to-one>
        <!-- bi-directional many-to-one association to AmtbAccount -->
        <key-many-to-one
        	name="amtbAccount"
	        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
	    >
    	    <column name="ACCOUNT_NO" />
	    </key-many-to-one>
    </composite-id>
    <version name="version" column="VERSION"/>

    <property
        name="effectiveDt"
        type="java.sql.Timestamp"
        column="EFFECTIVE_DT"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to MstbProdDiscMaster -->
    <many-to-one
        name="mstbProdDiscMaster"
        class="com.cdgtaxi.ibs.master.model.MstbProdDiscMaster"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="PRODUCT_DISCOUNT_PLAN_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to LrtbRewardMaster -->
    <many-to-one
        name="lrtbRewardMaster"
        class="com.cdgtaxi.ibs.master.model.LrtbRewardMaster"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="REWARD_PLAN_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to MstbSubscFeeMaster -->
    <many-to-one
        name="mstbSubscFeeMaster"
        class="com.cdgtaxi.ibs.master.model.MstbSubscFeeMaster"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="SUBSCRIPTION_FEE_NO" />
    </many-to-one>
    <!-- bi-directional many-to-one association to MstbIssuanceFeeMaster -->
    <many-to-one
        name="mstbIssuanceFeeMaster"
        class="com.cdgtaxi.ibs.master.model.MstbIssuanceFeeMaster"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ISSUANCE_FEE_NO" />
    </many-to-one>

</class>
</hibernate-mapping>
