<sql-query name="getTopBillGenReqNo">
<return-scalar column="top_req_no" type="integer"/>
 <![CDATA[
	select distinct first_value(req_no) over(order by hierachy desc) as top_req_no from (
		select req.*, level as hierachy
		from bmtb_bill_gen_req req
		start with req_no = :reqNo
		connect by prior  regen_req_no = req_no
	)
]]>
</sql-query>



