<class 
    name="com.cdgtaxi.ibs.common.model.FmtbGlLogHeader" 
    table="FMTB_GL_LOG_HEADER"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="glLogNo"
        type="java.lang.Integer"
        column="GL_LOG_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">FMTB_GL_LOG_HEADER_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="glDate"
        type="java.sql.Date"
        column="GL_DATE"
        length="7"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdDate"
        type="java.sql.Date"
        column="CREATED_DATE"
        length="7"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="fileDate"
        type="java.sql.Date"
        column="FILE_DATE"
        length="7"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to FmtbEntityMaster -->
    <many-to-one
        name="fmtbEntityMaster"
        class="com.cdgtaxi.ibs.common.model.FmtbEntityMaster"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ENTITY_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to FmtbGlLogDetail -->
    <set
        name="fmtbGlLogDetails"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="GL_LOG_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.FmtbGlLogDetail"
        />
    </set>

