<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class 
    name="com.cdgtaxi.ibs.common.model.IttbCpPymtReqReversal" 
    table="ITTB_CP_PYMT_REQ_REVERSAL"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

      <id
        name="reqRevId"
        type="java.lang.Integer"
        column="PYMT_REQ_REV_ID"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">ITTB_CP_PYMT_REQ_REVERSAL_SQ1</param>
        </generator>
    </id>   
	<version name="version" column="VERSION"/>

	
    <property
        name="reqStatus"
        type="java.lang.String"
        column="REQ_STATUS"
        length="3"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
  
    <property
        name="requestDt"
        type="java.sql.Timestamp"
        column="REQUESTED_DATE"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    <property
        name="requestBy"
        type="java.lang.String"
        column="REQUESTED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DATE"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
	<property
        name="sentCtr"
        type="java.lang.Integer"
        column="SENT_CTR"
        length="2"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    
    
       <property
        name="sentEmail"
        type="java.lang.String"
        column="SENT_EMAIL"
        length="3"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to PmtbProduct -->
    <many-to-one
        name="ittbCpPymtReqSum"
        class="com.cdgtaxi.ibs.common.model.IttbCpPymtReqSum"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="PYMT_REQ_ID" />
    </many-to-one>
  
    

</class>
</hibernate-mapping>
