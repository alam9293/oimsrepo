<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.common.model.LrtbGiftStock" 
    table="LRTB_GIFT_STOCK"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="giftStockNo"
        type="java.lang.Integer"
        column="GIFT_STOCK_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">LRTB_GIFT_STOCK_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="txnType"
        type="java.lang.String"
        column="TXN_TYPE"
        not-null="true"
        length="1"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="txnDt"
        type="java.sql.Timestamp"
        column="TXN_DT"
        not-null="true"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="txnQty"
        type="java.lang.Integer"
        column="TXN_QTY"
        length="9"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="serialNoStart"
        type="java.math.BigDecimal"
        column="SERIAL_NO_START"
        length="25"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="serialNoEnd"
        type="java.math.BigDecimal"
        column="SERIAL_NO_END"
        length="25"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="previousPts"
        type="java.lang.Integer"
        column="PREVIOUS_PTS"
        length="8"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="balancePts"
        type="java.lang.Integer"
        column="BALANCE_PTS"
        length="8"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to LrtbGiftItem -->
    <many-to-one
        name="lrtbGiftItem"
        class="com.cdgtaxi.ibs.common.model.LrtbGiftItem"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="GIFT_ITEM_NO" />
    </many-to-one>
    <!-- bi-directional one-to-many association to LrtbRewardTxn -->
    <set
        name="lrtbRewardTxns"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="GIFT_STOCK_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.LrtbRewardTxn"
        />
    </set>
    <!-- bi-directional many-to-one association to AmtbAccount -->
    <many-to-one
        name="amtbAccount"
        class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        not-null="true"
    >
        <meta attribute="use-in-equals">true</meta>
        <column name="ACCOUNT_NO" />
    </many-to-one>

</class>
</hibernate-mapping>
