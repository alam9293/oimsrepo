<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cdgtaxi.ibs.master.model.MstbBankMaster" 
    table="MSTB_BANK_MASTER"
    dynamic-update="true"
    dynamic-insert="true"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="bankMasterNo"
        type="java.lang.Integer"
        column="BANK_MASTER_NO"
    >
        <meta attribute="use-in-equals">true</meta>
        <generator class="sequence">
            <param name="sequence_name">MSTB_BANK_MASTER_SQ1</param>
        </generator>
    </id>
    <version name="version" column="VERSION"/>

    <property
        name="bankName"
        type="java.lang.String"
        column="BANK_NAME"
        not-null="true"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="bankCode"
        type="java.lang.String"
        column="BANK_CODE"
        not-null="true"
        length="4"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdDt"
        type="java.sql.Timestamp"
        column="CREATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedDt"
        type="java.sql.Timestamp"
        column="UPDATED_DT"
        length="11"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property
        name="updatedBy"
        type="java.lang.String"
        column="UPDATED_BY"
        length="80"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to BmtbPaymentReceipt -->
    <set
        name="bmtbPaymentReceipts"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="BANK" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.BmtbPaymentReceipt"
        />
    </set>
    <!-- bi-directional one-to-many association to MstbBranchMaster -->
    <set
        name="mstbBranchMasters"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="BANK_MASTER_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.master.model.MstbBranchMaster"
        />
    </set>
    <!-- bi-directional one-to-many association to AmtbBillReq -->
    <set
        name="amtbBillReqs"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="BANK_MASTER_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.AmtbBillReq"
        />
    </set>
    <!-- bi-directional one-to-many association to AmtbAccount -->
    <set
        name="amtbAccounts"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="BANK_MASTER_NO" />
        </key>
        <one-to-many 
            class="com.cdgtaxi.ibs.common.model.AmtbAccount"
        />
    </set>

</class>
</hibernate-mapping>
