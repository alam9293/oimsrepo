//from row number: 0
rule "Row 1 Eligiblity"
       dialect "mvel"
       when
              lOCIOSheetObject: LOCIOSheet(interest_subsidy_to_the_extent_of_5_p__per_annum_for_5_years_in_the_form_of_reimbursement_on_loan_taken_for_industrial_research_subject_to_an_overall_ceiling_of_Rs_1_Crore_QIS == "Yes" , overall_Eligibility_under_IIEPP_2017 == "Eligible" )
              
       then
              
              lOCIOSheetObject.setEligiblity("Eligible");
       end

