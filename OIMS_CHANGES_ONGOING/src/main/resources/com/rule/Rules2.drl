package com.rule

import com.oims.rule_creation.CIS
import com.oims.rule_creation.LOCIOSheet
import com.oims.rule_creation.model.Product



//from row number: 0
rule "Row 1 Eligiblity"
       dialect "mvel"
       when
              lOCIOSheetObject: LOCIOSheet(Eligibility_of_25_p_Gross_Block_in_case_of_expansion_diversification == "Not Eligible" )
              
       then
              
              lOCIOSheetObject.setCapital_Investment_in_Rs_including_New_Expansion_Diversification(0);
       end

rule "Row 2 Eligiblity"
       dialect "mvel"
       when
                 lOCIOSheetObject: LOCIOSheet(Eligibility_of_25_p_Gross_Block_in_case_of_expansion_diversification == "Eligible" )
              
       then
              
              lOCIOSheetObject.setCapital_Investment_in_Rs_including_New_Expansion_Diversification(3033000000);
       end
       
        


